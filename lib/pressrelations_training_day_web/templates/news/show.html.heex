<h1>Show News</h1>

<ul>

  <li>
    <strong>Headline:</strong>
    <%= @news.headline %>
  </li>

  <li>
    <strong>Text:</strong>
    <%= @news.text %>
  </li>

  <li>
    <strong>Publication:</strong>
    <%= @news.publication %>
  </li>

  <li>
    <strong>Date:</strong>
    <%= @news.date %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.news_path(@conn, :edit, @news) %></span> |
<span><%= link "Back", to: Routes.news_path(@conn, :index) %></span>
